# List of upstream proxy nodes (comma-separated)
PROXY_NODES=https://vpn-node-us.onrender.com,https://vpn-node-sg.onrender.com,https://vpn-node-eu.onrender.com

# Valid tokens for authentication
VALID_TOKENS=abc123

# Allowed origins (your website)
ALLOWED_ORIGINS=https://croak-pwa.vercel.app

# Rotation mode: round_robin or sticky_by_ip
ROTATION_MODE=round_robin

# Rate limit: max requests per minute
PROXY_RATE_LIMIT=120

# Server port
PORT=3000
